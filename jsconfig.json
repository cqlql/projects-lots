// https://code.visualstudio.com/docs/languages/jsconfig
{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": [
      "node_modules"
  ],
  "include": [
    "src/**/*.js",
    "src/**/*.vue"
  ]
}
